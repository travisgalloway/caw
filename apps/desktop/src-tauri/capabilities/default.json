{
  "$schema": "https://raw.githubusercontent.com/nicerdicer/tauri-v2-schema/refs/heads/main/src/acl.capability.v2.json",
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-execute",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "binaries/caw",
          "sidecar": true,
          "args": true
        }
      ]
    },
    "shell:allow-kill"
  ]
}
